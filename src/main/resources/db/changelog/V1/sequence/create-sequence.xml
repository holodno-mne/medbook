<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">

    <changeSet id="create-account_seq" author="holodno">
        <preConditions onFail="MARK_RAN">
            <not>
                <sequenceExists sequenceName="account_seq"/>
            </not>
        </preConditions>
        <createSequence sequenceName="account_seq" startValue="1"/>
        <rollback>
            <dropSequence sequenceName="account_seq"/>
        </rollback>
    </changeSet>

    <changeSet id="create-role_seq" author="holodno">
        <preConditions onFail="MARK_RAN">
            <not>
                <sequenceExists sequenceName="role_seq"/>
            </not>
        </preConditions>
        <createSequence sequenceName="role_seq" startValue="1"/>
        <rollback>
            <dropSequence sequenceName="role_seq"/>
        </rollback>
    </changeSet>

    <changeSet id="create-permission_seq" author="holodno">
        <preConditions onFail="MARK_RAN">
            <not>
                <sequenceExists sequenceName="permission_seq"/>
            </not>
        </preConditions>
        <createSequence sequenceName="permission_seq" startValue="1"/>
        <rollback>
            <dropSequence sequenceName="permission_seq"/>
        </rollback>
    </changeSet>

    <changeSet id="create-application_seq" author="holodno">
        <preConditions onFail="MARK_RAN">
            <not>
                <sequenceExists sequenceName="application_seq"/>
            </not>
        </preConditions>
        <createSequence sequenceName="application_seq" startValue="1"/>
        <rollback>
            <dropSequence sequenceName="application_seq"/>
        </rollback>
    </changeSet>

</databaseChangeLog>